SUPABASE_URL=tu_url_aqui
SUPABASE_KEY=tu_key_aqui

# Opcional: controlar páginas a scrapear (por defecto 1)
MAX_PAGES=1

# Modo incremental: procesa solo compras recientes (últimos ~75 minutos)
INCREMENTAL_MODE=true

# =============================
# ÓRDENES DE COMPRA (API)
# =============================
MP_API_TICKET=

# Rango de fechas OC (YYYY-MM-DD). Si MP_OC_TO se omite, usa MP_OC_FROM.
MP_OC_FROM=
MP_OC_TO=

# =============================
# LICITACIONES (API, >=100 UTM)
# =============================
# Rango YYYY-MM-DD. Si MP_LIC_TO se omite, usa MP_LIC_FROM.
MP_LIC_FROM=
MP_LIC_TO=

# (Opcional) filtrar por estado (ej: activas, adjudicada, etc.)
MP_LIC_ESTADO=

