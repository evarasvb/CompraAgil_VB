SUPABASE_URL=tu_url_aqui
SUPABASE_KEY=tu_key_aqui

# Opcional: controlar páginas a scrapear (por defecto 1)
MAX_PAGES=1

# Modo incremental: procesa solo compras recientes (últimos ~75 minutos)
INCREMENTAL_MODE=true

# =====================================================
# ÓRDENES DE COMPRA (OC) - Scraper API
# =====================================================
# Puedes usar un endpoint completo (recomendado) con placeholders:
# MP_OC_ENDPOINT=https://api.mercadopublico.cl/servicios/v1/publico/ordenesdecompra.json?ticket={ticket}&fecha_desde={from}&fecha_hasta={to}&pagina={page}
MP_OC_ENDPOINT=

# Si no defines MP_OC_ENDPOINT, el scraper intentará usar MP_API_TICKET con una URL default.
MP_API_TICKET=

# Rango de fechas default para OC (YYYY-MM-DD). Si están vacíos, se usa “hoy”.
MP_OC_FROM=
MP_OC_TO=

# Paginación (si el endpoint la soporta)
MP_OC_MAX_PAGES=10

