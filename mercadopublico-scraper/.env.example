SUPABASE_URL=tu_url_aqui
SUPABASE_KEY=tu_key_aqui

# Opcional: controlar páginas a scrapear (por defecto 1)
MAX_PAGES=1

# Modo incremental: procesa solo compras recientes (últimos ~75 minutos)
INCREMENTAL_MODE=true

# =====================================================
# ÓRDENES DE COMPRA (OC) - Scraper API
# =====================================================
# Endpoint oficial: se usa MP_API_TICKET (mismo ticket que licitaciones)
MP_API_TICKET=

# Rango de fechas default para OC.
# Recomendado: YYYY-MM-DD (ej: 2026-01-12). También soporta ddmmaaaa.
MP_OC_FROM=
MP_OC_TO=

# Nota: el endpoint oficial de listado es por fecha: fecha=ddmmaaaa.
# Ejemplo: 12012026 para 12 de enero 2026.

